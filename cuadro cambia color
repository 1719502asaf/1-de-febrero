function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(225, 0, 0);
  fill(255);
  rect(150, 150, 100, 100);
  if (mouseIsPressed && mouseX < 250 && mouseX > 150) {
    if (mouseY < 250 && mouseY > 150) {
      fill(255, 255, 0);
      rect(0, 0, 400, 400);
      fill(255);
      rect(150, 150, 100, 100);
    }
  }
}
